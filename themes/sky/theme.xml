        <name>Slide</name>
        <snippet>kind</snippet>
        <snippet>date</snippet>
    </category>
    <template>
        <name>None</name>
    </template>
    <template>
        <name>Contact</name>
        <snippet>phone</snippet>
        <snippet>address</snippet>
    </template>
